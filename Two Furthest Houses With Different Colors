class Solution {
    public int maxDistance(int[] c) {
        int n=c.length;
        int max=0;
        for(int i=0;i<n-1;i++){
            for(int j=i+1;j<n;j++){
                if(c[i]!=c[j]){
                int diff=j-i;
                max=Math.max(diff,max);
            }
        }
        }
        return max;
    }
}
