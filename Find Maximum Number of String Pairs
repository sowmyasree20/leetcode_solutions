class Solution {
    public int maximumNumberOfStringPairs(String[] w) {
        int c=0;
        for(int i=0;i<w.length;i++){
            for(int j=i+1;j<w.length;j++){
                String r=new StringBuilder(w[j]).reverse().toString();
                if(w[i].equals(r))
                   c++;
            }
        }
        return c;
    }
}
