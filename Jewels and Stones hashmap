class Solution {
    public int numJewelsInStones(String j, String s) {
        HashMap <Character,Integer> hm=new HashMap<>();
        for(int i=0;i<s.length();i++){
            char ch=s.charAt(i);
            hm.put(ch,hm.getOrDefault(ch,0)+1);
        }
        int ans=0;
        for(int i=0;i<j.length();i++){
            char ch=j.charAt(i);
            if(hm.containsKey(ch))
            ans+=hm.get(ch);
        }
        return ans;
    }
}
