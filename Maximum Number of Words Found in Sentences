class Solution {
    public int mostWordsFound(String[] s) {
        int t=0;
      
        for(int i=0;i<s.length;i++){
            int c=1;
            for(int j=0;j<s[i].length();j++){
                if(s[i].charAt(j)==' ')
                   c++;
            }
            t=Math.max(t,c);
        }
        return t;
    }
}
